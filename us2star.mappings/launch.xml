<?xml version="1.0"?> 

<project default="main">

 	<target name="loadModels">
 		<epsilon.emf.register file="../example/UsMetamodel.ecore"/>
 		<epsilon.emf.register file="../example/IstarMetamodel.ecore"/>
 		 		
 		<epsilon.emf.loadModel name="us"
                                       modelFile="us1.model"
                                       metamodelUri="us"
                                       read="true" store="false"/>
 		
 		<epsilon.emf.loadModel name="istar"
                                       modelFile="star1.model"
                                       metamodelUri="istar"
                                       read="false" store="true"/>
 	</target>
 	
	<target name="main" depends="loadModels">	
		
		<epsilon.etl src="us2star.etl">
			<model ref="us" as="Us"/>
			<model ref="istar" as="Istar"/>
		</epsilon.etl>
		
	</target>
</project>

